background-color:#ccc;
border-radius:5px;
position:relative;
height: 150px;
width: 850px;
float: left;
}
 
.hukidasi:after {
border:10px solid transparent;
border-right-color:#ccc;
border-left-width:0;
left:-10px;
content:"";
display:block;
top:30px;
position:absolute;
width:0;
}

